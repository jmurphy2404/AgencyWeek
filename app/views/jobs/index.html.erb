<p id="notice"><%= notice %></p>

<h1>Jobs</h1>

<table>
  <thead>
    <tr>
      <th>Origin lat</th>
      <th>Origin lng</th>
      <th>Dest lat</th>
      <th>Dest lng</th>
      <th>Origin</th>
      <th>Destination</th>
      <th>Date</th>
      <th>Time</th>
      <th>Bike</th>
      <th>Valet</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job.origin_lat %></td>
        <td><%= job.origin_lng %></td>
        <td><%= job.dest_lat %></td>
        <td><%= job.dest_lng %></td>
        <td><%= job.origin %></td>
        <td><%= job.destination %></td>
        <td><%= job.date %></td>
        <td><%= job.time %></td>
        <td><%= job.bike_id %></td>
        <td><%= job.valet_id %></td>
        <td><%= job.status %></td>
        <td><%= link_to 'Show', job %></td>
        <td><%= link_to 'Edit', edit_job_path(job) %></td>
        <td><%= link_to 'Destroy', job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job', new_job_path %>
