<!-- home landing page -->
<!-- Intro Header -->
<header class="masthead">
  <div class="intro-body">
    <div class="container">
      <div class="row">
        <div class="col-lg-8 mx-auto">
          <h1 class="brand-heading">Blitz</h1>

          <p class="intro-text">A way to get your bike anytime, anywhere.</p>
          <a href="#about" class="btn btn-circle js-scroll-trigger">
            <i class="fa fa-angle-double-down animated"></i>
          </a>
        </div>
      </div>
    </div>
  </div>
</header>

<!-- About Section -->
<section id="about" class="content-section text-center">
  <div class="container">
    <div class="row">
      <div class="col-lg-8 mx-auto">
        <%= link_to new_job_path do %>
        <i class="fa fa-bicycle fa-5x" aria-hidden="true"></i>
     </span>
  <% end %>

        <h2>About Blitz</h2>
        <p>Blitz is a service that gets your bike to you wherever you are, and when you aren't riding it, it's always in a safe place.
    </a>Here's how it works</p>
        <p>Just signup and
          <a href="http://googlemaps.com/">choose a pickup location</a>
          then we'll come get your bike and redeliver it to wherever you need it at any given time, rain or shine.

    </div>
  </div>
</section>

<!-- Download Section -->
<section id="download" class="download-section content-section text-center">
  <div class="container">
    <div class="col-lg-8 mx-auto">
      <h2>Love Riding Bikes? Come work for us!</h2>
      <p>Become one of our valets and ride bikes to make cash!</p>
      <a href="http://startbootstrap.com/template-overviews/grayscale/" class="btn btn-default btn-lg">Sign up on our Valet Page</a>
    </div>
  </div>
</section>



